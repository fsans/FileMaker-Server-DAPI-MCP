{
  "name": "filemaker-data-api-mcp",
  "version": "0.0.9",
  "description": "MCP server for FileMaker Data API integration with Claude Desktop",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "filemaker-mcp": "dist/bin/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "watch": "tsc --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "filemaker",
    "data-api",
    "rest-api",
    "claude",
    "claude-desktop"
  ],
  "author": "Francesc Sans <fsans@ntwk.es>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fsans/FileMaker-Server-DAPI-MCP"
  },
  "bugs": {
    "url": "https://github.com/fsans/FileMaker-Server-DAPI-MCP/issues"
  },
  "homepage": "https://github.com/fsans/FileMaker-Server-DAPI-MCP#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.7.9",
    "commander": "^12.1.0",
    "debug": "^4.4.3",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "form-data": "^4.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.5",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.7.3"
  }
}
